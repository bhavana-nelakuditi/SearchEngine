<script type="text/javascript">
	var publicEventId = "439360819037"
	var organizationID = "168615707214"
	var userId = ""
	var canEdit = "False"
	var eventDetailsFeatureGate = false
	jQuery(function() {
		var dimensions = [
			EB.Analytics.DIMENSIONS.EVENT_ID(publicEventId),
			EB.Analytics.DIMENSIONS.ACTIVE_SCOPE_USER_ID(organizationID)
		];
		if (userId) {
			dimensions.push(EB.Analytics.DIMENSIONS.USER_ID(userId));
		}
		dimensions.push(EB.Analytics.DIMENSIONS.EXPERIMENT_ID3(
			eventDetailsFeatureGate ? 'eventdetails:newdesign': 'eventdetails:control')
		);
		EB.Analytics.trackEvent({
			'action': 'ListingPageView',
			'label': canEdit === 'True' ? 'CanEdit' : 'CannotEdit',
			'category': 'listing',
			'dimensions': dimensions
		});
	});
	jQuery("#edit-event").click(function() {
		EB.Analytics.trackEvent({
			'action': 'EditEvent',
			'category': 'listing',
			'dimensions':
			userId ?
			[
				EB.Analytics.DIMENSIONS.EVENT_ID(publicEventId),
				EB.Analytics.DIMENSIONS.USER_ID(userId),
				EB.Analytics.DIMENSIONS.ACTIVE_SCOPE_USER_ID(organizationID),
			]
			:
			[
			EB.Analytics.DIMENSIONS.EVENT_ID(publicEventId),
			EB.Analytics.DIMENSIONS.ACTIVE_SCOPE_USER_ID(organizationID),
		]
		});
	});
	jQuery("#organizer-link-hero").click(function() {
		EB.Analytics.trackEvent({
			'action': 'OrganizerPageView',
			'category': 'listing',
			'label': 'EventDetails',
		});
	});
