				var isSafari = /^((?!chrome|android).)*safari/i.test(navigator.userAgent);
				if(isSafari) {
					function getOffset() {
						const diff = document.documentElement.clientHeight - window.innerHeight;
						return diff > 0 ? diff : 0;
					}
					const sticky = document.querySelectorAll('.js-sticky-panel')[0];
					if(sticky !== undefined) {
						function fixStickyBottomValue(stickyEl) {
							const offsetString = getOffset() + "px";
							if(stickyEl.style.bottom !== offsetString) {
								stickyEl.style.bottom = offsetString;
							}
						}
						window.addEventListener('resize', function(e) {
							window.requestAnimationFrame(function() {
								fixStickyBottomValue(sticky);
							});
						});
					}
				}
