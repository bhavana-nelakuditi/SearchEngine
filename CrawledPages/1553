    padding: 5px 8px;
    left: -5px;
    border-radius: 5px;
    white-space: break-spaces;
    overflow: hidden;
    text-overflow: ellipsis;
}

.content-body a {
    border-bottom: 0;
}

.em.em-calendar .em-cal-body.event-style-pill .em-cal-event > div {
    background: unset;
    color: black;
    color: #3672ad !important;
    border: 0;
    height: calc(100% + 10px);
    min-height: 28px;
    line-height: 20px;
    font-style: normal;
}



.em.em-calendar .em-cal-body.event-style-pill .em-cal-event {
    text-align: left;
    position: relative;
    padding: 0;
    overflow: hidden;
    margin: 5px 0;
    cursor: pointer;
    font-size: 14px;
    min-height: 28px;
    height: calc(100% + 10px);
	    margin-bottom: 1em;
}


.em.pixelbones .event-title h2{
  line-height: 26px!important;
}

.em.pixelbones h2 a {
  color: #3672ad !important;
  letter-spacing: 1px;
}

.em.pixelbones h2 a {
    font-family: "Lato",sans-serif;
    font-weight: 700;
    text-transform: none;
    font-size: .5em;
    margin-top: 0.55em;
    margin-bottom: 0.25em;
}

