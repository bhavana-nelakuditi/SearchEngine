<script>const uichomepageform = document.getElementById('uichomepageform')
    const uichomepageformState = {
        // if not empty, it will overwrite the redirect that happens at the end of form, no matter what other
        // logic says except for handleImmediateRedirectBasedOnFormValues
        finalRedirectOverwrite: null
    };
uichomepageform.addEventListener('freeform-render-success', function (event) {
                this.querySelector('.js-form-body')
            .remove();
    });

        uichomepageform.addEventListener('freeform-ready', function (event) {
        uichomepageform.querySelector('input[name=formHash]').value = '';

        window.fullstackacademy.initPhoneInputs(event.form);

        if (event.form.hasAttribute('data-gtm')) {
            window.fullstackacademy.gtmPushFormEventOnView(event.form)
            window.fullstackacademy.gtmPushFormEventsOnClick(event.form)
            window.fullstackacademy.gtmPushFormEventsOnFocusout(event.form)

            //window.fullstackacademy.gtmPushVisibleFields(event.form.elements);
            //window.fullstackacademy.gtmFormFieldFocusListener(event.form);
        }

                let tokenFetchInitialized = false
        function getTokenUponChange() {
            if (tokenFetchInitialized) {
                return
            }
            tokenFetchInitialized = true
            window.fullstackacademy.dynamicForm(uichomepageform, 'uicHomepageForm');
        }
        uichomepageform.addEventListener('input', getTokenUponChange);
        uichomepageform.addEventListener('change', getTokenUponChange);
        // temporarily always load form token upon load
        //getTokenUponChange()

        window.fullstackacademy.initializeDataPrefill();

        event.form.fullstackParams = new URLSearchParams();
        let params = new URLSearchParams(window.location.search.slice(1));
        params.forEach(function (param, key) {
            field = event.form.querySelector('[name=' + key + ']');
            if (field) {
                field.value = param;
            }
        });

        const formFieldsStorage = window.fullstackacademy.formFieldsStorage();
        // if provided in GET params, save start_date for later use on Apply P2 even though it's not present on P1 form.
        if (params.get('start_date')) {
            formFieldsStorage.storeField('start_date', params.get('start_date'));
        }

        let cohortsFieldsToPopulate = {}
        
                const cohortSelectorFunctionalityElement = uichomepageform.querySelector('.cohort-selector, .schedule-selector');
        if (cohortSelectorFunctionalityElement && cohortSelectorFunctionalityElement.dataset) {
            window.fullstackacademy.cohortSelectorSupport(uichomepageform, cohortsFieldsToPopulate)
        }

                const accStateSelectorElement = uichomepageform.querySelector('[fieldType="accStateSelector"]');
        if (accStateSelectorElement) {
            window.fullstackacademy.accMarketSelectorSupport(uichomepageform)
        }
        const formConfigElement = uichomepageform.querySelector('[name="hs_ignore_form_config"]');
        if (formConfigElement) {
            // TODO if needed: make a more general parser of value, with hide-XXX and possibly
            // other options
            if (formConfigElement.value == 'hide-acc-cyber') {
                const coursePreference = uichomepageform.querySelector('[name="course_preference"]')
                if (coursePreference) {
                    Array.from(coursePreference.options).forEach(option => {
                        if (option.value == 'acc-cyber') {
                            option.disabled = true
                            option.hidden = true
                            option.removeAttribute('selected')
                        }
                    })
                }
            }
        }

                window.fullstackacademy.formFieldsStorage(uichomepageform)

            });

        uichomepageform.addEventListener('freeform-on-submit', function (event) {
                document.getElementById('uichomepageformname').value = document.title;
        document.getElementById('uichomepageformhutk').value = window.fullstackacademy.getCookie('hubspotutk');
        document.getElementById('uichomepageformpageUri').value = window.location;

                const hubspotutk = event.form.querySelector('input[name="hubspotutk"]');

        if (hubspotutk) {
            hubspotutk.value = window.fullstackacademy.getCookie('hubspotutk');
        }
    });

    uichomepageform.addEventListener('freeform-ajax-error', function (event) {
        let currentStep = document.getElementById('uichomepageform-steps') ? document.getElementById('uichomepageform-steps').dataset.gtmStepCurrent : 'no step value';

        if (event.form.hasAttribute('data-gtm')) {
            window.fullstackacademy.gtmFormsEvent('error', currentStep);

            if (document.documentElement.scrollTop > event.form.offsetTop) {
                event.form.parentElement.scrollIntoView();
            }
        }
    });

    function doesFormCallbackExist(name) {
        return typeof window[`uichomepageform${name}`] === "function";
    }

        uichomepageform.addEventListener("freeform-ajax-before-submit", async function (event) {
        //formData
        window.fullstackacademy.setPrefillFormData(event);

        if (uichomepageform.querySelector('input[name=formHash]').value == '') {
            const triggerEvent = new Event('change')
            const eventTargetFreeform = event.target.freeform
            uichomepageform.dispatchEvent(triggerEvent)

            event.preventDefault()
            //eventTargetFreeform.unlockSubmit()

            const tokenCheckInterval = setInterval(function() {
                if (uichomepageform.querySelector('input[name=formHash]').value != '') {
                    eventTargetFreeform.triggerSubmit()
                    clearInterval(tokenCheckInterval)
                }
            }, 100)
        }

        const fields = event.form.querySelectorAll('[data-forward], [data-data-forward], [name="course_preference"], [name="rtc_schedule"]');

        window.fullstackacademy.formFieldsStorage().watchFieldsToStore(fields)

        if (window.fullstackacademy.handleImmediateRedirectBasedOnFormValues(uichomepageform, event)) {
            event.preventDefault()
            return
        }

        const finalRedirectOverwrite = window.fullstackacademy.getFinalRedirectOverwrite(uichomepageform)
        if (finalRedirectOverwrite) {
            uichomepageformState.finalRedirectOverwrite = finalRedirectOverwrite
        }

        const freeform = event.target.freeform;

        // TODO move to individual JS file
        const emailInputs = event.target.querySelectorAll('[type=email]')
        emailInputs.forEach((emailInput) => {
            //add custom error container if was not defined before
            if (!(uichomepageform.querySelector(`[data-for="${emailInput.id}"]`))) {
                const errorElement = document.createElement('div')
                errorElement.classList.add(`phone-input-error`)
                errorElement.setAttribute(`data-for`, emailInput.id)
                emailInput.after(errorElement)
            }

            const emailInputValue = emailInput.value.trim()
            const errorMsg = uichomepageform.querySelector(`[data-for="${emailInput.id}"]`)
            if (emailInputValue.length >= 1 || emailInput.hasAttribute('data-required')) {
                const emailCheckerResult = window.emailChecker(emailInputValue)
                if (emailCheckerResult.length == 0) {
                    errorMsg.innerHTML = ''
                } else {
                    errorMsg.innerHTML = `Invalid email. Did you mean <strong>${emailCheckerResult[0].corrected}</strong>?`
                    event.preventDefault()
                    freeform.unlockSubmit()
                }
            }
        })

        // TODO move to individual JS file
        const phoneInputs = event.target.querySelectorAll('[type=tel]')
        phoneInputs.forEach((phoneInput) => {
            const phoneInputValue = phoneInput.value.replace(/[ \-\(\)]/g, '')
            const errorMsg = uichomepageform.querySelector(`[data-for="${phoneInput.id}"]`)
            if (phoneInputValue.length >= 3 || phoneInput.hasAttribute('data-required')) {
                if (phoneInputValue.length >= 10 && phoneInputValue.length <= 15) {
                    errorMsg.innerHTML = ''
                } else {
                    errorMsg.innerHTML = 'Invalid number'
                    event.preventDefault()
                    freeform.unlockSubmit()
                }
            }
        })

        if (event.form.hasAttribute('data-gtm')) {
            window.fullstackacademy.gtmSaveFieldValuesForSubmit(event.form)
        }

            });

        uichomepageform.addEventListener('freeform-ajax-success', function (event) {

        if (doesFormCallbackExist('AjaxSuccessOverwrite')) {
            uichomepageformAjaxSuccessOverwrite(event);
        } else {
            window.fullstackacademy.initPhoneInputs(event.form)
            window.fullstackacademy.initializeDataPrefill()
            if (event.form.hasAttribute('data-gtm')) {
                window.fullstackacademy.gtmPushFormEventsOnClick(event.form)
                window.fullstackacademy.gtmPushFormEventsOnFocusout(event.form)
            }

                        const accStateSelectorElement = uichomepageform.querySelector('[fieldType="accStateSelector"]');
            if (accStateSelectorElement) {
                window.fullstackacademy.accMarketSelectorSupport(uichomepageform)
            }

                        const cohortSelectorFunctionalityElement = uichomepageform.querySelector('.cohort-selector, .schedule-selector');
            if (cohortSelectorFunctionalityElement && cohortSelectorFunctionalityElement.dataset) {
                window.fullstackacademy.cohortSelectorSupport(uichomepageform)
            }

            let prevStep = document.getElementById('uichomepageform-steps') ? document.getElementById('uichomepageform-steps').dataset.gtmStepPrevious : 'no step value';
            let completeStep = document.getElementById('uichomepageform-steps') ? document.getElementById('uichomepageform-steps').dataset.gtmStepComplete : 'no step value';

            if (document.documentElement.scrollTop > event.form.offsetTop) {
                event.form.parentElement.scrollIntoView();
            }

            // handleFreeformAjaxSuccessGtm
            if (event.form.hasAttribute('data-gtm')) {
                                if (event.response.success && event.response.finished) {
                    window.fullstackacademy.gtmFormsEvent('submit', completeStep);
                }

                                if (event.response.success && !event.response.finished) {
                    window.fullstackacademy.gtmFormsEvent('page-submit', prevStep);
                }

                                if (!event.response.success) {
                    window.fullstackacademy.gtmFormsEvent('error', prevStep);
                }
            }

                        if (event.response.returnUrl && event.response.returnUrl.length > 2) {
                if (event.form.hasAttribute('data-gtm')) {
                    window.fullstackacademy.gtmPushFormEventsOnSubmit(event.form)
                }

                let returnUrl = event.response.returnUrl

                if (uichomepageformState.finalRedirectOverwrite) {
                    // overwrite redirect based on previously executed conditionals
                    returnUrl = uichomepageformState.finalRedirectOverwrite
                }

                queryString = event.form.fullstackParams.toString();
                window.location.href = returnUrl + (queryString ? '?' + queryString : '');
            }
        }
    });
const uichomepagemodalform = document.getElementById('uichomepagemodalform')
    const uichomepagemodalformState = {
        // if not empty, it will overwrite the redirect that happens at the end of form, no matter what other
        // logic says except for handleImmediateRedirectBasedOnFormValues
        finalRedirectOverwrite: null
    };
uichomepagemodalform.addEventListener('freeform-render-success', function (event) {
                this.querySelector('.js-form-body')
            .remove();
    });

        uichomepagemodalform.addEventListener('freeform-ready', function (event) {
        uichomepagemodalform.querySelector('input[name=formHash]').value = '';

        window.fullstackacademy.initPhoneInputs(event.form);

        if (event.form.hasAttribute('data-gtm')) {
            window.fullstackacademy.gtmPushFormEventOnView(event.form)
            window.fullstackacademy.gtmPushFormEventsOnClick(event.form)
            window.fullstackacademy.gtmPushFormEventsOnFocusout(event.form)

            //window.fullstackacademy.gtmPushVisibleFields(event.form.elements);
            //window.fullstackacademy.gtmFormFieldFocusListener(event.form);
        }

                let tokenFetchInitialized = false
        function getTokenUponChange() {
            if (tokenFetchInitialized) {
                return
            }
            tokenFetchInitialized = true
            window.fullstackacademy.dynamicForm(uichomepagemodalform, 'uicHomepageModalForm');
        }
        uichomepagemodalform.addEventListener('input', getTokenUponChange);
        uichomepagemodalform.addEventListener('change', getTokenUponChange);
        // temporarily always load form token upon load
        //getTokenUponChange()

        window.fullstackacademy.initializeDataPrefill();

        event.form.fullstackParams = new URLSearchParams();
        let params = new URLSearchParams(window.location.search.slice(1));
        params.forEach(function (param, key) {
            field = event.form.querySelector('[name=' + key + ']');
            if (field) {
                field.value = param;
            }
        });

        const formFieldsStorage = window.fullstackacademy.formFieldsStorage();
        // if provided in GET params, save start_date for later use on Apply P2 even though it's not present on P1 form.
        if (params.get('start_date')) {
            formFieldsStorage.storeField('start_date', params.get('start_date'));
        }

        let cohortsFieldsToPopulate = {}
        
                const cohortSelectorFunctionalityElement = uichomepagemodalform.querySelector('.cohort-selector, .schedule-selector');
        if (cohortSelectorFunctionalityElement && cohortSelectorFunctionalityElement.dataset) {
            window.fullstackacademy.cohortSelectorSupport(uichomepagemodalform, cohortsFieldsToPopulate)
        }

                const accStateSelectorElement = uichomepagemodalform.querySelector('[fieldType="accStateSelector"]');
        if (accStateSelectorElement) {
            window.fullstackacademy.accMarketSelectorSupport(uichomepagemodalform)
        }
        const formConfigElement = uichomepagemodalform.querySelector('[name="hs_ignore_form_config"]');
        if (formConfigElement) {
            // TODO if needed: make a more general parser of value, with hide-XXX and possibly
            // other options
            if (formConfigElement.value == 'hide-acc-cyber') {
                const coursePreference = uichomepagemodalform.querySelector('[name="course_preference"]')
                if (coursePreference) {
                    Array.from(coursePreference.options).forEach(option => {
                        if (option.value == 'acc-cyber') {
                            option.disabled = true
                            option.hidden = true
                            option.removeAttribute('selected')
                        }
                    })
                }
            }
        }

                window.fullstackacademy.formFieldsStorage(uichomepagemodalform)

            });

        uichomepagemodalform.addEventListener('freeform-on-submit', function (event) {
                document.getElementById('uichomepagemodalformname').value = document.title;
        document.getElementById('uichomepagemodalformhutk').value = window.fullstackacademy.getCookie('hubspotutk');
        document.getElementById('uichomepagemodalformpageUri').value = window.location;

                const hubspotutk = event.form.querySelector('input[name="hubspotutk"]');

        if (hubspotutk) {
            hubspotutk.value = window.fullstackacademy.getCookie('hubspotutk');
        }
    });

    uichomepagemodalform.addEventListener('freeform-ajax-error', function (event) {
        let currentStep = document.getElementById('uichomepagemodalform-steps') ? document.getElementById('uichomepagemodalform-steps').dataset.gtmStepCurrent : 'no step value';

        if (event.form.hasAttribute('data-gtm')) {
            window.fullstackacademy.gtmFormsEvent('error', currentStep);

            if (document.documentElement.scrollTop > event.form.offsetTop) {
                event.form.parentElement.scrollIntoView();
            }
        }
    });

    function doesFormCallbackExist(name) {
        return typeof window[`uichomepagemodalform${name}`] === "function";
    }

        uichomepagemodalform.addEventListener("freeform-ajax-before-submit", async function (event) {
        //formData
        window.fullstackacademy.setPrefillFormData(event);

        if (uichomepagemodalform.querySelector('input[name=formHash]').value == '') {
            const triggerEvent = new Event('change')
            const eventTargetFreeform = event.target.freeform
            uichomepagemodalform.dispatchEvent(triggerEvent)

            event.preventDefault()
            //eventTargetFreeform.unlockSubmit()

            const tokenCheckInterval = setInterval(function() {
                if (uichomepagemodalform.querySelector('input[name=formHash]').value != '') {
                    eventTargetFreeform.triggerSubmit()
                    clearInterval(tokenCheckInterval)
                }
            }, 100)
        }

        const fields = event.form.querySelectorAll('[data-forward], [data-data-forward], [name="course_preference"], [name="rtc_schedule"]');

        window.fullstackacademy.formFieldsStorage().watchFieldsToStore(fields)

        if (window.fullstackacademy.handleImmediateRedirectBasedOnFormValues(uichomepagemodalform, event)) {
            event.preventDefault()
            return
        }

        const finalRedirectOverwrite = window.fullstackacademy.getFinalRedirectOverwrite(uichomepagemodalform)
        if (finalRedirectOverwrite) {
            uichomepagemodalformState.finalRedirectOverwrite = finalRedirectOverwrite
        }

        const freeform = event.target.freeform;

        // TODO move to individual JS file
        const emailInputs = event.target.querySelectorAll('[type=email]')
        emailInputs.forEach((emailInput) => {
            //add custom error container if was not defined before
            if (!(uichomepagemodalform.querySelector(`[data-for="${emailInput.id}"]`))) {
                const errorElement = document.createElement('div')
                errorElement.classList.add(`phone-input-error`)
                errorElement.setAttribute(`data-for`, emailInput.id)
                emailInput.after(errorElement)
            }

            const emailInputValue = emailInput.value.trim()
            const errorMsg = uichomepagemodalform.querySelector(`[data-for="${emailInput.id}"]`)
            if (emailInputValue.length >= 1 || emailInput.hasAttribute('data-required')) {
                const emailCheckerResult = window.emailChecker(emailInputValue)
                if (emailCheckerResult.length == 0) {
                    errorMsg.innerHTML = ''
                } else {
                    errorMsg.innerHTML = `Invalid email. Did you mean <strong>${emailCheckerResult[0].corrected}</strong>?`
                    event.preventDefault()
                    freeform.unlockSubmit()
                }
            }
        })

        // TODO move to individual JS file
        const phoneInputs = event.target.querySelectorAll('[type=tel]')
        phoneInputs.forEach((phoneInput) => {
            const phoneInputValue = phoneInput.value.replace(/[ \-\(\)]/g, '')
            const errorMsg = uichomepagemodalform.querySelector(`[data-for="${phoneInput.id}"]`)
            if (phoneInputValue.length >= 3 || phoneInput.hasAttribute('data-required')) {
                if (phoneInputValue.length >= 10 && phoneInputValue.length <= 15) {
                    errorMsg.innerHTML = ''
                } else {
                    errorMsg.innerHTML = 'Invalid number'
                    event.preventDefault()
                    freeform.unlockSubmit()
                }
            }
        })

        if (event.form.hasAttribute('data-gtm')) {
            window.fullstackacademy.gtmSaveFieldValuesForSubmit(event.form)
        }

            });

        uichomepagemodalform.addEventListener('freeform-ajax-success', function (event) {

        if (doesFormCallbackExist('AjaxSuccessOverwrite')) {
            uichomepagemodalformAjaxSuccessOverwrite(event);
        } else {
            window.fullstackacademy.initPhoneInputs(event.form)
            window.fullstackacademy.initializeDataPrefill()
            if (event.form.hasAttribute('data-gtm')) {
                window.fullstackacademy.gtmPushFormEventsOnClick(event.form)
                window.fullstackacademy.gtmPushFormEventsOnFocusout(event.form)
            }

                        const accStateSelectorElement = uichomepagemodalform.querySelector('[fieldType="accStateSelector"]');
            if (accStateSelectorElement) {
                window.fullstackacademy.accMarketSelectorSupport(uichomepagemodalform)
            }

                        const cohortSelectorFunctionalityElement = uichomepagemodalform.querySelector('.cohort-selector, .schedule-selector');
            if (cohortSelectorFunctionalityElement && cohortSelectorFunctionalityElement.dataset) {
                window.fullstackacademy.cohortSelectorSupport(uichomepagemodalform)
            }

            let prevStep = document.getElementById('uichomepagemodalform-steps') ? document.getElementById('uichomepagemodalform-steps').dataset.gtmStepPrevious : 'no step value';
            let completeStep = document.getElementById('uichomepagemodalform-steps') ? document.getElementById('uichomepagemodalform-steps').dataset.gtmStepComplete : 'no step value';

            if (document.documentElement.scrollTop > event.form.offsetTop) {
                event.form.parentElement.scrollIntoView();
            }

            // handleFreeformAjaxSuccessGtm
            if (event.form.hasAttribute('data-gtm')) {
                                if (event.response.success && event.response.finished) {
                    window.fullstackacademy.gtmFormsEvent('submit', completeStep);
                }

                                if (event.response.success && !event.response.finished) {
                    window.fullstackacademy.gtmFormsEvent('page-submit', prevStep);
                }

                                if (!event.response.success) {
                    window.fullstackacademy.gtmFormsEvent('error', prevStep);
                }
            }

                        if (event.response.returnUrl && event.response.returnUrl.length > 2) {
                if (event.form.hasAttribute('data-gtm')) {
                    window.fullstackacademy.gtmPushFormEventsOnSubmit(event.form)
                }

                let returnUrl = event.response.returnUrl

                if (uichomepagemodalformState.finalRedirectOverwrite) {
                    // overwrite redirect based on previously executed conditionals
                    returnUrl = uichomepagemodalformState.finalRedirectOverwrite
                }

                queryString = event.form.fullstackParams.toString();
                window.location.href = returnUrl + (queryString ? '?' + queryString : '');
            }
        }
    });</script></body></html>
